{
  "parser"  : "babel-eslint",
  "extends" : [
    "plugin:import/errors",
    "plugin:import/warnings",
    "react-app"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "settings": {
    "import/core-modules": ["electron"],
    "import/extensions": [".js", ".jsx", ".electron.js", ".web.js", ".native.js", ".ios.js", ".android.js"]
  },
  "rules": {
    "import/no-named-as-default": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "import/default": 0
  }
}
